class Program
    {
        static void Main(string[] args)
        {
            int _sum = 10;
            Toplam(10, 20, 30 out _sum);

            Console.WriteLine(_sum);
        }


        static int Toplam(int num1, int num2, num3 out int sum)
        {
            sum = num1 + num2 + num3;
            return sum;
        }
    }
